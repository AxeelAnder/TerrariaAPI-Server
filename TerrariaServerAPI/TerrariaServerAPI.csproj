<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <LangVersion>7.3</LangVersion>
		<Company>Re-Logic &amp; The TShock Team</Company>
		<Copyright>Copyright © Re-Logic 2017</Copyright>
		<Product>Terraria</Product>
		<Title>Terraria</Title>
		<AssemblyName>TerrariaServerApi</AssemblyName>
		<RootNamespace>TerrariaServerApi</RootNamespace>
		<AssemblyVersion>1.3.5.3</AssemblyVersion>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="tshock_logo.ico" />
    <None Remove="tshock_logo.png" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="OTAPI, Version=1.3.5.3, Culture=neutral, PublicKeyToken=null">
      <HintPath>../TShock.Modifications.Bootstrapper/bin/$(Configuration)/Output/OTAPI.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.1" />
    <PackageReference Include="NLog" Version="4.7.0" />
    <PackageReference Include="System.Diagnostics.PerformanceCounter" Version="4.7.0" />
    <PackageReference Include="System.IO.Packaging" Version="4.7.0" />
  </ItemGroup>
</Project>
